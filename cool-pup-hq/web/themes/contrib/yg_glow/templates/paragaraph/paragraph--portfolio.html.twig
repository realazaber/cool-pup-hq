<!-- Testimonials -->
<div id="fh5co-portfolio">
	<div class="container">
		<div class="row">
			<div class="col-md-6 col-md-offset-3 text-center heading-section animate-box">
				<h3>{{ content.field_title.0 }}</h3>
				{{ content.field_body.0 }}
			</div>
		</div>
		<div class="row row-bottom-padded-md">
			<div class="col-md-12">
				<ul id="fh5co-portfolio-list">
					{{ content.field_portfolio_items }}
				</ul>		
			</div>
		</div>
		{% if content.field_url.0|render is not empty %}
			<div class="row">
				<div class="col-md-4 col-md-offset-4 text-center animate-box">
					<a href="{{ content.field_url[0]['#url']|render }}" class="btn btn-primary btn-lg">{{ content.field_url[0]['#title'] }}</a>
				</div>
			</div>
		{% endif %}	
	</div>
</div>
<!-- Testimonials End-->
