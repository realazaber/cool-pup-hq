	<div class="container">
		<div class="row">
			<div class="col-md-8 col-md-offset-2 text-center heading-section animate-box">
				<h3>{{ content.field_title.0 }}</h3>
				{{ content.field_body.0 }}
			</div>
		</div>
	</div>
	<div class="container">
		<div class="row row-bottom-padded-md">
			<div class="col-md-6 animate-box">
				<figure>
					<img src="{{ file_url(content.field_image['#items'].entity.uri.value) }}" alt="{{ content.field_phone_number.0 }}" class="img-responsive">
